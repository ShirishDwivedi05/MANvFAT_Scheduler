<script type="text/javascript">

    $("#photoTab").on('click', function () {
        //alert('');
        LoadPlayerImages();
    });
    function LoadPlayerImages() {
        var str = '';
        var strInner = '';
        var IsDefaultImage = true;
        $.ajax({
            url: '/PlayerImages/GetPlayerImagesAsync',
            type: 'GET',
            data: { id: '@Model.PlayerID' },
            success: function (result) {
                //var returnVal = result.playerId;
                if (result != null) {
                    for (var i = 0; i < result.length; i++) {
                        if (i == 8) {
                            break;
                        }
                        str = str + "<div class='col-lg-3  col-md-3 col-sm-3 col-xs-3 p1'><div class='modal-toggle' id='" + result[i].PlayerImageID + "' onclick='OepnPopup(" + result[i].PlayerImageID + ")'><img src='" + result[i].AbsoluteImageLink + "' style='height:100px!important; padding-bottom :10px !important;' class='img-responsive' alt='img'><input type='button' class='delete' onclick='removeImage(" + result[i].PlayerImageID + ")' /></div></div>";
                    }
                    for (var i = 0; i < result.length; i++) {
                        if (result[i].AbsoluteImageLink == result[i].AbsoluteImageLink) {
                            if (IsDefaultImage) {
                                strInner = strInner + "<div class='item active'><img src='"+result[i].AbsoluteImageLink+"' style='height:230px;width:299px' class='img-responsive' alt='img'></div>";
                                IsDefaultImage = false;
                            }
                            else {
                                strInner = strInner + "<div class='item'><img style='height: 230px; width: 299px' src='"+ result[i].AbsoluteImageLink + "' class='img-responsive' alt='img'></div>";
                            }

                        }
                        else {
                            strInner = strInner + "<div class='item'><img src='" + result[i].AbsoluteImageLink + "' class='img-responsive' alt='img'></div>";
                        }
                    }
                    $("#divPlayerPhotos").empty();
                    $(".carousel-inner").empty();
                    $("#divPlayerPhotos").html(str);
                    $(".carousel-inner").html(strInner);
                }
            },
            error: function () {
                //alert("error");
            }
         });
    }
    function removeImage(id) {
        $.ajax({
            url: '/PlayerImages/DeleteImage',
            type: 'GET',
            data: { PlayerImageID: id,PlayerId: '@Model.PlayerID' },
            success: function (result) {
                //var returnVal = result.playerId;
                if (result != null) {
                    LoadPlayerImages();
                }
            },
            error: function () {
                //alert("error");
            }
        });
    }

    function OepnPopup(id) {
        $('#' + id).parents('.photo_aft-bef,.photo_libraryPic').find('.modal-bf-aft').addClass('is-visible', function () {
            $(this).parents('.before-after,.libry-photo').addClass('modal-opned');
        });

    }
   
</script>
