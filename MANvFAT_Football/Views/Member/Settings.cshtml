<script>
    function Edit(id) {
        $("." + id + "_lbl").addClass("hidden");

        if (id == "clsName") {
            $(".clsFirstName_txt").removeClass("hidden");
            $(".clsLastName_txt").removeClass("hidden");
            $(".clsFirstName_txt").select().focus();
            $("." + id + "_btnEdit").addClass("hidden");
            $("." + id + "_btnSave").removeClass("hidden");
        }
        
        else {
            $("." + id + "_lbl").addClass("hidden");
            $("." + id + "_txt").removeClass("hidden");
            $("." + id + "_txt").select().focus();
            $("." + id + "_btnEdit").addClass("hidden");
            $("." + id + "_btnSave").removeClass("hidden");
        }
    }

    function Save(id) {
        var NewValue = "";
        if (id == "clsName") {
            NewValue = $(".clsFirstName_txt").val() + " " + $(".clsLastName_txt").val();
            $("." + id + "_lbl").text(NewValue);
            $(".clsFirstName_txt").addClass("hidden");
            $(".clsLastName_txt").addClass("hidden");

            HideShow_Controls(id);
        }
        else if (id == "clsHeight")
        {
          //  alert("height = " + $("." + id + "_txt").children('option:selected').data("height"));
            NewValue = $("." + id + "_txt").children('option:selected').data("ft");
            $("." + id + "_lbl").text(NewValue+"\"");

            HideShow_Controls(id);
        }
        else if (id == "clsStartingWeight") {
            //  alert("height = " + $("." + id + "_txt").children('option:selected').data("height"));
            NewValue = $("." + id + "_txt").children('option:selected').data("lbs");
            $("." + id + "_lbl").text(NewValue);

            HideShow_Controls(id);
        }
        else {
            NewValue = $("." + id + "_txt").val();
            $("." + id + "_lbl").text(NewValue);

            HideShow_Controls(id);
        }

        //TODO: Now Create Full Object with Values


    }

    function HideShow_Controls(id) {
        $("." + id + "_lbl").removeClass("hidden");
        $("." + id + "_txt").addClass("hidden");
        $("." + id + "_btnEdit").removeClass("hidden");
        $("." + id + "_btnSave").addClass("hidden");
    }

</script>

