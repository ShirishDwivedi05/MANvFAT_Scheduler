@model List<MANvFAT_Football.Models.SelectPlayer_PecentAchievementCategoryCompleted_Dashboard_Result>

    <div class="row clsAchievementCategories">
        @foreach (var item in Model)
        {
            string cls = "p" + item.PercentCompleted.Value.ToString("N0");

            <div class="col-xs-4 clsAchievementCategory" title="Click to show your Unlocked and Locked @item.AchievementCategory Achievements" onclick="return ShowCategory_Locked_Unlocked_Achievements('@item.AchievementCategoryID')">
                <div class="tile data-circle">
                    <div class="c100 @cls green">
                        <span>@item.PercentCompleted.Value.ToString("N0")%</span>
                        <div class="slice">
                            <div class="bar"></div>
                            <div class="fill"></div>
                        </div>
                    </div>
                    <h4 class="text-center">@item.AchievementCategory</h4>
                </div>
            </div>
        }
    </div>
